.title {
  width: 100%;
  text-align: center;
  padding: 10px 0;
}
.title{
  height: 50rpx;
}
.back{
  width: 114rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  background-color: #cccc;
  position: absolute;
  top:9rpx;
  left:25rpx;

}
.list {
  margin-top: 100rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: left;
  font-size: 30rpx;
}

.list>.box {
  width: 185rpx;
  height: 185rpx;
  /* border: 1px solid #cccc; */
  margin: 30rpx 31rpx;
  position: relative;
  text-align: center;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-radius: 20rpx;
  
}
.list>.box .icon{
  margin: 0 auto;
}
.list text{
  display: inline-block;
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.list>.box navigator {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 10;
}

.list>.box text {
  position: absolute;
  bottom: -9rpx;
  left: 50%;
  transform: translateX(-50%);
}

.list>.box ico {
  margin-top: 10rpx;
}

.tabarcontent {
  position:fixed;
  bottom: 0rpx;
  width: 100%;
}
.icon{
  font-size: 78rpx;
  margin: 0 auto;
}

