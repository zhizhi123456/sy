/* pages/project/project.wxss */
.cover-tip-wrapper {
    margin: 20rpx 30rpx 0;
    height: 100rpx;
    color: #6d6c6c;
    font-weight: bold;
    border-radius: 20rpx;
}

.tip-text {
    max-width: 82%;
    line-height: 100rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-size: 36rpx;
    text-align: center;
    background-color: rgba(250, 250, 250, 0.9);
    border-radius: 20rpx;
    margin-left: 130rpx;
}

.return {
    width: 100rpx;
    height: 100rpx;
    background-color: rgba(250, 250, 250, 0.9);
    border-radius: 50%;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
    display: flex;
    justify-content: space-around;
}

.return .iconfont {
    font-size: 64rpx;
    font-weight: bold;
    line-height: 100rpx;
    color: teal;
}